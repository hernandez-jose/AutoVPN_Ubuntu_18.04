#!/usr/bin/expect
spawn ./build-key-server server
expect "Country*:"
send "\r"
expect "State*:"
send "\r"
expect "Locality*:"
send "\r"
expect "Organization*:"
send "\r"
expect "Organizational*:"
send "\r"
expect "Common*:"
send "\r"
expect "Name*:"
send "\r"
expect "Email*:"
send "\r"
expect "A challenge password*:"
send "\r"
expect "An optional*:"
send "\r"
expect "Sign the certificate*:"
send "y\r"
expect "*certificate requests certified*"
send "y\r"
expect eof
